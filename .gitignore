# Core .gitignore
#
# This file is intended to prevent overwriting of important core items and
# ensure that extra unnecessary files do not make it into the repository.

########################################################################
# Drupal specific                                                      #
########################################################################
config/drupal/local-settings.php
config/drupal/proxy-settings.php
config/drupal/proxy-settings-disabled.php

########################################################################
