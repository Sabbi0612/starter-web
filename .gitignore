########################################################################
.DS_Store
.AppleDouble
.LSOverride
Icon
._*
*.orig

# Files that might appear on external disk
.Spotlight-V100
.Trashes

########################################################################
# Jetbrains products (webstorm, phpstorm, etc.)                        #
########################################################################
.idea/

########################################################################
# Sublime Text                                                         #
########################################################################
/*.sublime-project
*.sublime-workspace

########################################################################
# SVN                                                                  #
########################################################################
.svn/
.svn-*

########################################################################
# Textmate                                                             #
########################################################################
*.tmproj
*.tmproject
tmtags

########################################################################
# Vim                                                                  #
########################################################################
.*.sw[a-z]
*.un~
Session.vim
.netrwhist

########################################################################
